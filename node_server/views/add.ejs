    <h1>Enter info</h1>
    <form method="POST" action="/client">
	<div class="form-group">   
            <label for="public_key">Public Key:</label>
            <input class="form-control" type="text" name="public_key" placeholder="Enter Public Key">
        </div>
	<div class="form-group">
	    <label for="vpn_ip">VPN IP:</label>
            <input class="form-control" type="text" name="vpn_ip" placeholder="Enter VPN IP">
	</div>
	<button type="submit" class="btn btn-success btn-block">Add Peer</button>
    </form>
